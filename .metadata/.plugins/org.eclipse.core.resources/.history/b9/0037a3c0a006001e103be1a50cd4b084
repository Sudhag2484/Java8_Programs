package in.sudha.streamapi;

import java.util.stream.Stream;

public class ParallelDemo {

	public static void main(String[] args) {

		System.out.println("=============Serial Stream==============");
		Stream<Integer> ss = Stream.of(1,3,5,6,8,2);
		ss.forEach(n->System.out.println(n+"----"+Thread.currentThread()));
		
		
		System.out.println("==============Paralell Stream=============");
		Stream<Integer> ps = Stream.of(1,3,5,6,8,2);
		ps.parallel().forEach(n->System.out.println(););
	}

}
